            <div class="row" style="padding-top:15px;">


                <div class="col-sm-2" style="padding-left:0">
                    <label for="data-plenouso">Pleno Uso</label>
                    <input type="text" id="pleno-uso" name="pleno-uso" onkeypress="mascara(this, mdata);" size="14" maxlength="10" class="form-control" placeholder="00/00/0000" />
                </div>

                <div class="col-sm-2">
                    <label for="data-comunicacao">Data Comunicação</label>
                    <input type="text" id="data-comunicacao" name="data-comunicacao" onkeypress="mascara(this, mdata);" size="14" maxlength="10" class="form-control" placeholder="99/99/9999" />
                </div>

                <div class="col-sm-2">
                    <label for="data-visita">Data visita</label>
                    <input type="text" id="data-visita" name="data-visita" onkeypress="mascara(this, mdata);" size="14" maxlength="10" class="form-control" placeholder="99/99/9999" />
                </div>

                <div class="col-sm-2">
                    <label for="agencia-visitada">Agência visitada</label>
                    <input type="text" id="agencia-visitada" name="agencia-visitada" class="form-control" />
                </div>

                <div class="col-sm-2">
                    <label for="responsavel-neg">Contato Negócio</label>
                    <input type="text" id="responsavel-neg" name="responsavel-neg" class="form-control" />
                </div>

                <div class="col-sm-2">
                    <label for="responsavel-ds">Contato DS</label>
                    <input type="text" id="responsavel-ds" name="responsavel-ds" class="form-control" />
                </div>

            </div>

            <div>
                <button type="button" class="btn-toggle" data-element="#minhaDiv">Mostrar / Esconder</button>

            </div>

            <div id="minhaDiv" class="row" style="padding-top:15px; display:none">

                @{Html.RenderAction("_DetalhesIniciativa", "Iniciativa");}


            </div>

            <div class="row" style="padding-top:15px;">

                <div class="col-md-3" style="border: 1px solid #ccc; border-image: none; width: 210.5px; padding-right: 0px; margin-right: 15px;">
                    <div>
                        <label for="form-parcial-etapa" style=" padding-left: 15px; color:brown"> Origem:</label>
                    </div>

                    @{Html.RenderAction("_ListaEtapas", "Etapa");}

                </div>

                <div class="col-md-3" style=" border: 1px solid #ccc; width: 210.5px;">

                    <div>
                        <label for="form-parcial-etapa" style=" padding-left: 15px; color:brown"> Vertical:</label>
                    </div>
                    @{Html.RenderAction("_ListaEtapas", "Etapa");}

                </div>


                <div class="col-md-7" style="width: 732.46px;">
                    <label style="margin-left:18px">Resumo:</label>
                    <textarea rows="9" class="form-control" style="min-width: 100%;"></textarea>
                </div>

            </div>






        </form>
    </div>

</body>
</html>

<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/JS/iniciativa.js"></script>
<script src="~/Scripts/JS/maskaras.js"></script>

<script>
    $(function () {
        $('[data-toggle="tooltip"]').tooltip()
    })

    $(function () {
        $(".btn-toggle").click(function (e) {
            e.preventDefault();
            el = $(this).data('element');
            $(el).toggle();
        });
    });
</script>








