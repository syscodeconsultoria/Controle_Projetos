@model NovoControleProjetos.Models.Iniciativa


@{
    // Layout = null;
    var texto = "Orçamento";
}




<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

    <div class="container" style="padding-top:15px">
        <form>

            <div class="row">

                <div class="col-md-2" style="padding-right: 0px; padding-left: 0px;">
                    <label for="iniciativa">Iniciativa</label>
                    <input type="text" id="iniciativa" name="iniciativa" class="form-control" />

                </div>

                <div class="col-md-6">
                    <label for="nome-projeto">Nome Projeto</label>
                    <input type="text" class="form-control" style="min-width: 100%;" />
                    @*<input type="text" id="nome-projeto" name="nome-projeto" data-id="@Model.Id_Iniciativa" class="form-control" value="@Model.Nome_Iniciativa" />*@
                </div>

                <div class="col-md-2" style="padding-left:0">
                    <label for="inpuDepartamento">Departamento</label>
                    @{Html.RenderAction("_ListaDepartamentos", "Departamento");}
                </div>

                <div class="col-md-2">
                    <label for="data-aprovacao">Data Aprovação</label>
                    <input type="text" id="data-aprovacao" name="data-aprovacao" onkeypress="mascara(this, mdata);" size="14" maxlength="10" class="form-control" />
                </div>
            </div>


            <div class="row" style="padding-top:15px;">

                <div class="col-md-2">

                    <div class="row">
                        <label for="cores">Farol</label>
                        @{ Html.RenderAction("_ListaFarol", "Farol");}

                    </div>
                    <div class="row">
                        <label for="inputEsteira">Esteira</label>
                        @{Html.RenderAction("_ListaEsteiras", "Esteira");}
                    </div>

                    <div class="row">
                        <label for="inputEsteira">Esteira</label>
                        @{Html.RenderAction("_ListaEsteiras", "Esteira");}
                    </div>

                </div>

                <div class="col-md-6" style="">

                    <div class="row">

                        <label style="margin-left:18px">Comentário</label>
                        <textarea rows="8" class="form-control" style="margin-left: 17px; min-width: 90%;"></textarea>

                    </div>

                </div>


                <div class="col-md-2" style="padding-left: 0px; margin-left: 0px;">
                    @*<label for="form-parcial-orcamento" style=" padding-left: 15px; color:brown" data-toggle="tooltip" data-placement="left" title="@texto"> Orçamento:</label>*@
                    @{Html.RenderAction("_Create", "Orcamento");}

                </div>

                <div class="col-md-2" style="width: 180px; padding-left: 0px;">

                    <div class="row">
                        <label for="cpf-iniciativa">CPF</label>
                        <input type="text" id="cpf-iniciativa" name="cpf-iniciativa" class="form-control" />
                    </div>

                    <div class="row">
                        <label for="total_contratado">Total Aprovado CETI</label>
                        <input type="text" id="total_contratado" name="total_contratado" onkeypress="mascara( this, mvalor );" maxlength="14" class="form-control" />
                    </div>

                    <div class="row">
                        <label for="data-ceti">Data CETI</label>
                        <input type="text" id="data-ceti" name="data-ceti" onkeypress="mascara(this, mdata);" size="14" maxlength="10" class="form-control" />
                    </div>

                </div>

            </div>

            <div class="row" style="padding-top:15px;">

                <div class="col-sm-2" style="padding-left:0">
                    <label for="data-piloto">Piloto</label>
                    <input type="text" id="data-piloto" name="data-piloto" onkeypress="mascara(this, mdata);" size="14" maxlength="10" class="form-control" placeholder="00/00/0000" />
                </div>

                <div class="col-sm-2">
                    <label for="tf-versao-agencia">TF Agencia</label>
                    <input type="text" id="tf-versao-agencia" name="tf-versao-agencia" class="form-control" />
                </div>

                <div class="col-sm-2">
                    <label for="tf-versao-pa">TF PA</label>
                    <input type="text" id="tf-versao-pa" name="tf-versao-pa" class="form-control" />
                </div>

                <div class="col-md-2">
                    <label for="ux">UX</label>
                    <select id="ux" class="form-control">
                        <option value="none"> - </option>
                        <option value="true">Sim</option>
                        <option value="false">Não</option>
                    </select>
                </div>



                <div class="col-md-2">
                    <label for="ux">Jornada foi analisada?</label>
                    <select id="jornada-analisada" class="form-control">
                        <option value="none"> - </option>
                        <option value="true">Sim</option>
                        <option value="false">Não</option>
                    </select>
                </div>

                <div class="col-md-2">
                    <label for="inputEsteira">Mega</label>
                    @{Html.RenderAction("_ListaEsteiras", "Esteira");}
                </div>

            </div>


